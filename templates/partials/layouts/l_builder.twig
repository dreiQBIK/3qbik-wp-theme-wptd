{% set modules = post.meta('builder_content') %}
{% for module in modules %}
   {# Module: HEADLINE #}
   {% if module.acf_fc_layout == 'm_heading' %}
      {% include "/partials/modules/builder/m_headline.twig" with {
         headline: module.m_heading.text,
         space_bottom: module.m_heading.margin
      }%}
   {% endif %}
   {# Module: TEXT #}
   {% if module.acf_fc_layout == 'm_editor' %}
      {% include "/partials/modules/builder/m_text.twig" with {
         editor: module.m_editor.wysiwyg,
         space_bottom: module.m_editor.margin
      }%}
   {% endif %}
{% endfor %}
