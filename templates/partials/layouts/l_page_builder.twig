
{% set modules = post.get_field('pb') %}

{% for module in modules %}

    {# Module: HEADLINE #}
    {% if module.acf_fc_layout == 'pb_headline' %}
        {% include "/partials/modules/page_builder/m_headline.twig"%}
    {% endif %}

    {# Module: TEXT #}
    {% if module.acf_fc_layout == 'pb_editor' %}
        {% include "/partials/modules/page_builder/m_text.twig"%}
    {% endif %}

{% endfor %}
